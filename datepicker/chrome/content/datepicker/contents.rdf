<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

	<RDF:Seq RDF:about="urn:mozilla:package:root">
		<RDF:li RDF:resource="urn:mozilla:package:@EXTENSION@"/>
	</RDF:Seq>

	<RDF:Seq RDF:about="urn:mozilla:overlays">
		<RDF:li RDF:resource="chrome://browser/content/browser.xul"/>
		<RDF:li RDF:resource="chrome://navigator/content/navigator.xul"/>
	</RDF:Seq>

	<RDF:Seq RDF:about="chrome://browser/content/browser.xul">
		<RDF:li>chrome://@EXTENSION@/content/@EXTENSION@Overlay.xul</RDF:li>
	</RDF:Seq>

	<RDF:Seq about="chrome://navigator/content/navigator.xul">
		<RDF:li>chrome://@EXTENSION@/content/@EXTENSION@Overlay.xul</RDF:li>
	</RDF:Seq>

	<RDF:Description RDF:about="urn:mozilla:package:@EXTENSION@"
		chrome:displayName="@EXTENSION_FULL@ @VERSION@"
		chrome:author="@AUTHOR@"
		chrome:authorURL="@BASE_URL@"
		chrome:name="@EXTENSION@"
		chrome:extension="true"
		chrome:settingsURL="chrome://@EXTENSION@/content/@EXTENSION@Prefs.xul"
		chrome:description="@DESCRIPTION@">
	</RDF:Description>

</RDF:RDF>
